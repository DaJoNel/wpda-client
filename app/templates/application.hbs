<div class="container-fluid">
	{{outlet}}
	<h2>List of Places</h2>
	{{#if model.length}}
	<table class="table-responsive table-striped table-condensed">
		<thead>
			<tr>
				<th>Verified?</th>
				<th>Name</th>
				<th>Lock level</th>
				<th>Categories</th>
				<th>Number</th>
				<th>Street</th>
				<th>City</th>
				<th>State</th>
				<th>Country</th>
				<th>Updated by</th>
				<th>Updated on</th>
				<th>User report on</th>
				<th>Residential?</th>
			</tr>
		</thead>
		<tbody>
		{{#each model as |place|}}
			<tr>
				<td>{{place.isVerified}}</td>
				<td><a href="{{place.permalink}}">{{place.name}}</a></td>
				<td>{{place.lockLevel}}</td>
				<td>{{place.categories}}</td>
				<td>{{place.number}}</td>
				<td>{{place.street}}</td>
				<td>{{place.city}}</td>
				<td>{{place.state}}</td>
				<td>{{place.country}}</td>
				<td>{{place.updatedBy}}</td>
				<td>{{place.updatedOn}}</td>
				<td>{{place.userReportOn}}</td>
				<td>{{place.isResidential}}</td>
			</tr>
		{{/each}}
		</tbody>
	</table>
	{{else}}
	<p>No Places found</p>
	{{/if}}
</div>